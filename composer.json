{
  "name": "medunes/cache-billing",
  "description": "Cache billing system",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": [
    "parse",
    "xml",
    "ods",
    "pdf",
    "odt"
  ],
  "homepage": "https://github.com/medunes/cache-billing",
  "authors": [
    {
      "name": "medunes",
      "email": "contact@medunes.net",
      "homepage": "https://github.com/medunes"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/annotations": "^1.11",
    "dompdf/dompdf": "^1.0",
    "knplabs/knp-snappy": "^1.2",
    "league/pipeline": "^1.0",
    "mpdf/mpdf": "^8.0",
    "phpdocumentor/reflection-docblock": "^5.4",
    "symfony/console": "5.4.*",
    "symfony/css-selector": "5.4.*",
    "symfony/dom-crawler": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/finder": "5.4.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.4.*",
    "symfony/monolog-bundle": "^3.6",
    "symfony/process": "5.4.*",
    "symfony/property-access": "5.4.*",
    "symfony/property-info": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/twig-bundle": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/yaml": "5.4.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "pdepend/pdepend": "^2.8",
    "phpmd/phpmd": "^2.9",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/debug-bundle": "^5.4",
    "symfony/maker-bundle": "^1.29",
    "symfony/phpunit-bridge": "^5.4",
    "symfony/stopwatch": "^5.4",
    "symfony/var-dumper": "^5.4",
    "symfony/web-profiler-bundle": "^5.4",
    "vimeo/psalm": "^4.2"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "setup:clean": "rm -f composer.lock && rm -rf vendor/ && composer clearcache && composer update --no-interaction --prefer-dist --no-suggest --no-progress",
    "phpcs:scan": "php vendor/bin/php-cs-fixer fix --verbose --dry-run --stop-on-violation --using-cache=no --config=.php_cs.dist src/",
    "phpcs:fix": "php vendor/bin/phpcbf",
    "build:phpunit": "php vendor/bin/phpunit -c phpunit.xml --coverage-clover coverage.xml",
    "build:phpstan": [
      "phpstan analyze src --level=5 --configuration=phpstan.neon"
    ],
    "build:psalm": "vendor/bin/psalm",
    "build": [
      "@phpcs:scan",
      "@build:phpunit",
      "@build:phpstan",
      "@build:psalm"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.4.*"
    }
  }
}
